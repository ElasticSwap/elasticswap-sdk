{
  "name": "@elastic-dao/elasticswap-sdk",
  "version": "0.0.1",
  "description": "SDK for ElasticSwap",
  "main": "dist/index.js",
  "repository": "https://github.com/elasticdao/elasticswap-sdk",
  "author": "0xean",
  "license": "GPL-3.0",
  "private": false,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "analyzer": "yarn stats && yarn webpack-bundle-analyzer stats.json",
    "build": "npx webpack -c ./webpack.config.production.js",
    "dev": "yarn lint && npx webpack -c ./webpack.config.js --watch",
    "lint": "yarn prettier && npx eslint src/*.js src/**/*.js src/**/**/*.js src/**/**/**/*.js",
    "prerelease": "yarn build",
    "prettier": "npx prettier --write src/*.js --write src/**/*.js --write src/**/**/*.js --write src/**/**/**/*.js",
    "release": "npm publish --access=public",
    "stats": "npx webpack -c ./webpack.config.js --json > stats.json",
    "test": "mocha './**/*test.js"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "mocha": "^9.1.3",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "ethers": "^5.5.1",
    "rxjs": "^7.4.0"
  }
}
