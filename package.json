{
  "name": "@elasticswap/sdk",
  "version": "0.0.8",
  "description": "Official SDK for ElasticSwap",
  "main": "dist/index.js",
  "repository": "https://github.com/elasticswap/elasticswap-sdk",
  "author": "ElasticSwap",
  "license": "GPL-3.0",
  "private": false,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "analyzer": "yarn stats && yarn webpack-bundle-analyzer stats.json",
    "build": "npx webpack -c ./webpack.config.production.js",
    "dev": "yarn lint && npx webpack -c ./webpack.config.js --watch",
    "docs": "npx jsdoc2md -c .jsdoc.json src/index.mjs > dist/readme.md",
    "lint": "yarn prettier && npx eslint src/*.mjs src/**/*.mjs test/**/*.mjs test/**/*.js",
    "prerelease": "yarn build",
    "prettier": "npx prettier --write src/*.mjs --write src/**/*.mjs --write test/**/*.mjs --write test/**/*.js",
    "release": "npm publish --access=public",
    "stats": "npx webpack -c ./webpack.config.js --json > stats.json",
    "test": "mocha -timeout 10000 -exit './test/**/**.test.mjs' --experimental-json-modules"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/register": "^7.15.3",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "babel-loader": "^8.2.3",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.3.4",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "esm": "^3.2.25",
    "hardhat": "^2.6.7",
    "hardhat-deploy": "^0.9.4",
    "jsdoc-to-markdown": "^7.1.1",
    "mocha": "^9.1.3",
    "node-fetch": "^3.0.0",
    "prettier": "^2.4.1",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@elasticswap/elasticswap": "^0.0.5",
    "bignumber.js": "^9.0.1",
    "bnc-notify": "^1.9.1",
    "ethers": "^5.5.1",
    "rxjs": "^7.4.0"
  }
}
